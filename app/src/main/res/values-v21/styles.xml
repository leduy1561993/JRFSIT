<resources>

    <!-- Base application theme. -->
    <style name="AppThemeNoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@color/colorbackgroupwhite</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="selectableItemBackground">@drawable/state_list_backgroud</item>
        <item name="colorControlActivated">@color/colorPrimary</item>
    </style>
    <style name="foodRatingBar" parent="@style/Base.Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/food_rating_bar_full</item>
        <item name="android:minHeight">30dip</item>
        <item name="android:maxHeight">30dip</item>
    </style>
</resources>
